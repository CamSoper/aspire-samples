{
  "resources": {
    "cache": {
      "type": "container.v0",
      "connectionString": "{cache.bindings.tcp.host}:{cache.bindings.tcp.port}",
      "image": "redis:7.2.4",
      "bindings": {
        "tcp": {
          "scheme": "tcp",
          "protocol": "tcp",
          "transport": "tcp",
          "containerPort": 6379
        }
      }
    },
    "apiservice": {
      "type": "project.v0",
      "path": "../HealthChecksUI.ApiService/HealthChecksUI.ApiService.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "HEALTHCHECKSUI_URLS": "{apiservice.bindings.http.url}/healthz",
        "AllowedHosts": "{apiservice.bindings.http.host};{apiservice.bindings.https.host}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "webfrontend": {
      "type": "project.v0",
      "path": "../HealthChecksUI.Web/HealthChecksUI.Web.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "ConnectionStrings__cache": "{cache.connectionString}",
        "HEALTHCHECKSUI_URLS": "{webfrontend.bindings.healthchecksui.url}/healthz",
        "AllowedHosts": "{webfrontend.bindings.http.host};{webfrontend.bindings.https.host};{webfrontend.bindings.healthchecksui.host}",
        "services__apiservice__0": "{apiservice.bindings.http.url}",
        "services__apiservice__1": "{apiservice.bindings.https.url}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http"
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        },
        "healthchecksui": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http"
        }
      }
    },
    "healthchecksui": {
      "type": "container.v0",
      "image": "xabarilcoding/healthchecksui:5.0.0",
      "env": {
        "ui_path": "/",
        "HealthChecksUI__HealthChecks__0__Name": "apiservice",
        "HealthChecksUI__HealthChecks__0__Uri": "{apiservice.bindings.http.url}/healthz",
        "HealthChecksUI__HealthChecks__1__Name": "webfrontend",
        "HealthChecksUI__HealthChecks__1__Uri": "{webfrontend.bindings.healthchecksui.url}/healthz"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "containerPort": 80
        }
      }
    }
  }
}